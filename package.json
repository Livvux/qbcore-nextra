{
  "name": "qbcore-nextra",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "next dev",
    "build": "pnpm run typecheck && next build",
    "build:production": "next build",
    "build:static": "pnpm run typecheck && NEXT_OUTPUT=export next build",
    "export": "pnpm run build:static",
    "build:seo": "pnpm run build && pnpm run seo:validate",
    "start": "node start.js",
    "start:dev": "next start",
    "start:standalone": "node .next/standalone/server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "precommit": "pnpm run lint && pnpm run typecheck && pnpm run test --run",
    "clean": "rm -rf .next node_modules coverage",
    "check": "pnpm run lint && pnpm run typecheck",
    "check-all": "pnpm run lint && pnpm run typecheck && pnpm run test --run",
    "analyze": "ANALYZE=true pnpm build",
    "seo:validate": "tsx scripts/validate-seo.ts",
    "seo:test": "curl -I https://qbcore.net/sitemap.xml || echo 'Sitemap validation: Please check sitemap accessibility after deployment'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Livvux/qbcore-nextra.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Livvux/qbcore-nextra/issues"
  },
  "homepage": "https://github.com/Livvux/qbcore-nextra#readme",
  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/mdx": "^2.0.13",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.539.0",
    "next": "^14.2.31",
    "nextra": "^3.3.1",
    "nextra-theme-docs": "^3.3.1",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.31",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "24.3.0",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.31",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
